{
  "name": "markdown-it-shiki-extra",
  "type": "module",
  "version": "0.1.4",
  "packageManager": "pnpm@7.16.0",
  "description": "Markdown It plugin for Shiki with extra options",
  "author": {
    "name": "Young Liang",
    "email": "younggglcy@gmail.com"
  },
  "license": "MIT",
  "homepage": "https://github.com/GODLiangCY/markdown-it-shiki-extra/#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/GODLiangCY/markdown-it-shiki-extra.git"
  },
  "bugs": {
    "url": "https://github.com/GODLiangCY/markdown-it-shiki-extra/issues"
  },
  "keywords": [
    "markdown-it",
    "shiki",
    "markdown-it-plugin",
    "highlight"
  ],
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  },
  "sideEffects": false,
  "exports": {
    ".": {
      "require": "./dist/index.cjs",
      "import": "./dist/index.js",
      "node": {
        "require": "./dist/index.cjs",
        "import": "./dist/index.js",
        "default": "./dist/index.cjs"
      },
      "default": "./dist/index.cjs"
    }
  },
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "tsup --watch src/",
    "build": "tsup",
    "lint": "eslint .",
    "lint:fix": "pnpm lint --fix",
    "publish": "pnpm publish --access=public",
    "release": "bumpp && pnpm publish",
    "prepublishOnly": "pnpm build"
  },
  "peerDependencies": {
    "ts-node": "^10.9.1"
  },
  "dependencies": {
    "shiki": "^0.14.1",
    "synckit": "^0.8.5"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^0.38.4",
    "@types/markdown-it": "^12.2.3",
    "@types/node": "^18.15.11",
    "bumpp": "^9.1.0",
    "eslint": "^8.38.0",
    "tsup": "^6.7.0",
    "typescript": "^5.0.4"
  }
}
